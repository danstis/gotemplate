{
    "dryRun": false,
    "debug": true,
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/github",
            {
                "assets": [
                    {
                        "path": "artifacts/example-linux-x86/example",
                        "label": "example-linux-x86"
                    },
                    {
                        "path": "artifacts/example-win-x64/example.exe",
                        "label": "example-win-x64"
                    }
                ]
            }
        ]
    ]
}
